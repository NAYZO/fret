        <div class="col-md-4"> 
            <div class="row">
                <h4 class="title-banner" style="">Profil</h4>
            </div>
        </div>
    </div>
{% endblock banner %}
{% block content %}
<div class="row">
<div class="panel panel-default bottom col-md-4 col-md-offset-4">
    <div class="panel-body">
        <img src="{{ asset('clientfilesupload/')}}{{user.logoname}}" width="200" height="100" alt="alll" class="img-thumbnail bottom col-md-offset-2">                      

             
                    <label>Nom Entreprise: <span>{{user.nomentrop}}</span></label>
           
                    <label>Ville : <span>{{user.ville}}</span></label>
          
                    <label>Téléphone fix : <span>{{user.tel}}</span></label>
              
                    <label>adresse : <span>{{user.adresse}}</span></label>
               
