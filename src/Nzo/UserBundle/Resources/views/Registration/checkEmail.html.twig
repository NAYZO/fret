{% extends "NzoTunisiefretBundle::layout.html.twig" %}

{% block title %} {{ parent() }} Confirmation {% endblock %}
{% block banner %}
    <div class="jumbotron">
        <div class="col-md-4 col-sm-11 col-xs-11"> 
            <div class="row">
                <h4 class="title-banner" style="">Confirmation D'inscription</h4>
            </div>
        </div>
    </div>
{% endblock banner %}
{% block content %}   
<div class="bg-login" style="margin-top: 115px;margin-bottom: 155px;">
    <div class="alert alert-info text-center">
        <h4><strong>{{ 'registration.check_email'|trans({'%email%': user.email}, 'FOSUserBundle') }}</strong></h4>
    </div>
</div>
{% endblock content %}
    
         