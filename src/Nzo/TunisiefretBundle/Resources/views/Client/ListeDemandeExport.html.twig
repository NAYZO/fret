{% extends "NzoTunisiefretBundle::layoutClient.html.twig" %}
{% block title %} {{ parent() }} Client {% endblock %}

{% block banner %}
<div class="jumbotron">
    <div class="col-md-4"> 
        <div class="row">
            <h4 class="title-banner" style="">Liste Des Demandes Export </h4>
        </div>
    </div>
</div>
{% endblock banner %}

{% block content %}
<div class="col-md-8 col-md-offset-2 well">
<table class="table table-striped table-bordered">
        <thead>
          <tr>
            <th>Titre</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
            {% for demande in listedemandeexport %}
          <tr>
            <td>{{demande.titre}}</td>
            <td>
                <a href="{{path('nzo_detaildemande_export', {'id':demande.id} )}}"> Voir detail</a>
            </td>
         
          </tr>
             {% endfor %}
        </tbody>
      </table>

    
    <div class="navigation">
    {{ knp_pagination_render(listedemandeexport) }}
    </div>
    </div> 
{% endblock content %} 
 