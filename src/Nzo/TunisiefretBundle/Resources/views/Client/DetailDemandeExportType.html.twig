{% extends "NzoTunisiefretBundle::layoutClient.html.twig" %}
{% block title %} {{ parent() }} Détails Demandes Export Type {% endblock %}
{% block banner %}
<div class="jumbotron">
    <div class="col-md-4"> 
        <div class="row">
            <h4 class="title-banner" style="">Détails Demandes Export Type</h4>
        </div>
    </div>
</div>
{% endblock banner %}

{% block content %}
<div class="legend">
    <h4>{{mydemande.titre}} </h4>
    <ul class="nav nav-pills cl-effect-17">
        <li><a href="{{path('client_reposter_demande_type', {'id':mydemande.id} )}}"> Reposter Demande Type</a></li>
        <li><a href="{{path('client_supprimer_demande_type', {'id':mydemande.id} )}}"> Supprimer Demande Type</a></li>
    </ul>
</div>
    <div class="col-md-6">
        <p><strong>Réf : </strong>{{mydemande.reference}} </p>
        <p><strong>Prix : </strong>{{mydemande.prix}} </p>
        <p><strong>Pays : </strong>{{mydemande.pays}} </p>
        <p><strong>Ville : </strong>{{mydemande.ville}} </p>
    </div>
    
    <div class="col-md-6">
        <p><strong>Code Postal : </strong>{{mydemande.codepostal}} </p>
        <p><strong>Adresse : </strong>{{mydemande.adresse}} </p>
        <p><strong>Date Maximal : </strong>{{mydemande.datemax|date("d/m/Y")}} </p>
        <p><strong>Date Pos : </strong>{{mydemande.datedepos|date("d/m/Y")}} </p>
    </div>
    <div class="">
    <p><strong>Description : </strong>{{mydemande.description}} </p>
    </div>

{% endblock content %}
 
 